;;; Commentary:

;; A port of the popular Vim theme Zenburn for Emacs 24, built on top
;; of the new built-in theme support in Emacs 24.

;;; Credits:

;; Jani Nurminen created the original theme for vim on such this port
;; is based.

;;; Code:

(deftheme displat "The displat color theme")

;;; Color Palette

(let
   (("displat-fg"            . "#DDDDDD")
    ("displat-fg-1"          . "#999999")
    ("displat-bg"            . "#2b2b2b")
    ("displat-red"           . "#CC4242")
    ("displat-orange"        . "#FFBB45")
    ("displat-yellow"        . "#FAFA02")
    ("displat-dark-green"    . "#66F73E")
    ("displat-cyan"          . "#93E0E3")
    ("displat-blue"          . "#3C9093")
    ("displat-magenta"       . "#DC8CC3"))
  "List of Displat colors.
Each element has the form (NAME . HEX).

`+N' suffixes indicate a color is lighter.
`-N' suffixes indicate a color is darker.")

(defmacro displat-with-color-variables (&rest body)
  "`let' bind all colors defined in `displat-colors-alist'.
Also bind `class' to ((class color) (min-colors 89))."
  (declare (indent 0))
  `(let ((class '((class color) (min-colors 89)))
         ,@(mapcar (lambda (cons)
                     (list (intern (car cons)) (cdr cons)))
                   displat-colors-alist))
     ,@body))

;;; Theme Faces
(displat-with-color-variables
  (custom-theme-set-faces
   'displat
;;;; Built-in
;;;;; basic coloring
   '(button ((t (:underline t))))
