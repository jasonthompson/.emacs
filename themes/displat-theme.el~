;;; Rainbow Support

(declare-function rainbow-mode 'rainbow-mode)
(declare-function rainbow-colorize-by-assoc 'rainbow-mode)

(defvar displat-add-font-lock-keywords nil
  "Whether to add font-lock keywords for displat color names.
In buffers visiting library `displat-theme.el' the displat
specific keywords are always added.  In all other Emacs-Lisp
buffers this variable controls whether this should be done.
This requires library `rainbow-mode'.")

(defvar displat-colors-font-lock-keywords nil)

;; (defadvice rainbow-turn-on (after displat activate)
;;   "Maybe also add font-lock keywords for displat colors."
;;   (when (and (derived-mode-p 'emacs-lisp-mode)
;;              (or displat-add-font-lock-keywords
;;                  (equal (file-name-nondirectory (buffer-file-name))
;;                         "displat-theme.el")))
